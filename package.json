{
  "name": "wave-ui",
  "version": "1.33.0",
  "description": "An emerging UI framework for Vue.js & Vue 3 with only the bright side. :sunny:",
  "author": "Antoni Andre <antoniandre.web@gmail.com>",
  "main": "./dist/wave-ui.umd.js",
  "unpkg": "dist/wave-ui.umd.js",
  "jsdelivr": "dist/wave-ui.umd.js",
  "module": "./dist/wave-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/wave-ui.es.js",
      "require": "./dist/wave-ui.umd.js"
    }
  },
  "files": [
    "dist",
    "src/wave-ui"
  ],
  "keywords": [
    "wave ui",
    "ui framework",
    "ui library",
    "vue ui",
    "vue3",
    "vue framework",
    "ui"
  ],
  "license": "MIT",
  "repository": "https://github.com/antoniandre/wave-ui",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base /wave-ui/",
    "build-bundle": "BUNDLE=true vite build && mv ./dist/style.css ./dist/wave-ui.css",
    "serve": "vite preview --root docs --base /wave-ui/",
    "lint": "vite lint"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.vue"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.4",
    "@mdi/font": "^5.9.55",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.11",
    "autoprefixer": "^10.2.6",
    "axios": "^0.21.1",
    "core-js": "^3.13.1",
    "eslint": "^7.27.0",
    "eslint-plugin-vue": "^7.10.0",
    "font-awesome": "^4.7.0",
    "ghspa": "^1.0.0",
    "gsap": "^3.6.1",
    "ionicons": "^4.6.3",
    "material-design-icons": "^3.0.1",
    "node-sass": "^4.14.1",
    "rollup-plugin-delete": "^2.0.0",
    "sass": "^1.34.1",
    "simple-syntax-highlighter": "^1.4.5",
    "splitpanes": "^2.3.6",
    "vite": "^2.3.6",
    "vite-plugin-pug": "^0.3.0",
    "vite-plugin-vue2": "^1.6.2",
    "vue": "^2.6.13",
    "vue-router": "^3.5.1",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.13",
    "vueperslides": "^2.15.0"
  }
}
