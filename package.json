{
  "name": "wave-ui",
  "version": "2.20.0",
  "description": "An emerging UI framework for Vue.js & Vue 3 with only the bright side. :sunny:",
  "author": "Antoni Andre <antoniandre.web@gmail.com>",
  "main": "./dist/wave-ui.umd.js",
  "unpkg": "dist/wave-ui.umd.js",
  "jsdelivr": "dist/wave-ui.umd.js",
  "module": "./dist/wave-ui.es.js",
  "exports": {
    "./dist/wave-ui.css": "./dist/wave-ui.css",
    "./dist/wave-ui.cjs.js": "./dist/wave-ui.cjs.js",
    ".": {
      "import": "./dist/wave-ui.es.js",
      "require": "./dist/wave-ui.umd.js"
    }
  },
  "files": [
    "dist",
    "src/wave-ui"
  ],
  "keywords": [
    "wave ui",
    "ui framework",
    "ui library",
    "vue ui",
    "vue3",
    "vue framework",
    "ui"
  ],
  "license": "MIT",
  "repository": "https://github.com/antoniandre/wave-ui",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base /wave-ui/",
    "build-bundle": "BUNDLE=true vite build && mv ./dist/style.css ./dist/wave-ui.css",
    "serve": "vite preview --root docs --base /wave-ui/",
    "lint": "vite lint"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.vue"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.4",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@mdi/font": "^5.9.55",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "3.1.2",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "eslint": "^7.28.0",
    "font-awesome": "^4.7.0",
    "gsap": "^3.6.1",
    "ionicons": "^4.6.3",
    "material-design-icons": "^3.0.1",
    "rollup-plugin-delete": "^2.0.0",
    "sass": "^1.34.0",
    "simple-syntax-highlighter": "^2.0.6",
    "splitpanes": "^3.0.4",
    "vite": "^2.3.3",
    "vite-plugin-pug": "^0.3.0",
    "vue": "^3.1.2",
    "vue-cal": "^4.2.0",
    "vue-router": "^4.0.8",
    "vueperslides": "^3.3.0"
  }
}
