{
  "name": "wave-ui",
  "version": "2.19.3",
  "description": "An emerging UI framework for Vue.js & Vue 3 with only the bright side. :sunny:",
  "author": "Antoni Andre <antoniandre.web@gmail.com>",
  "main": "./dist/wave-ui.umd.js",
  "unpkg": "dist/wave-ui.umd.js",
  "jsdelivr": "dist/wave-ui.umd.js",
  "module": "./dist/wave-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/wave-ui.es.js",
      "require": "./dist/wave-ui.umd.js"
    }
  },
  "files": [
    "dist",
    "src/wave-ui"
  ],
  "keywords": [
    "wave ui",
    "ui framework",
    "ui library",
    "vue ui",
    "vue3",
    "vue framework",
    "ui"
  ],
  "license": "MIT",
  "repository": "https://github.com/antoniandre/wave-ui",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base /wave-ui/",
    "build-bundle": "BUNDLE=true vite build && mv ./dist/style.css ./dist/wave-ui.css",
    "serve": "vite preview --root docs --base /wave-ui/",
    "lint": "vite lint"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.vue"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.4",
    "@mdi/font": "^5.9.55",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.12.1",
    "eslint": "^7.26.0",
    "eslint-plugin-vue": "^7.9.0",
    "font-awesome": "^4.7.0",
    "ghspa": "^1.0.0",
    "gsap": "^3.6.1",
    "ionicons": "^4.6.3",
    "material-design-icons": "^3.0.1",
    "node-sass": "^6.0.0",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^10.2.0",
    "simple-syntax-highlighter": "^2.0.5",
    "splitpanes": "^3.0.3",
    "vue": "^3.0.11",
    "vue-router": "^4.0.8",
    "vue-svg-loader": "^0.16.0",
    "vueperslides": "^3.3.0"
  }
}
