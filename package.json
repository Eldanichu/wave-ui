{
  "name": "wave-ui",
  "version": "1.41.1",
  "description": "An emerging UI framework for Vue.js & Vue 3 with only the bright side. :sunny:",
  "author": "Antoni Andre <antoniandre.web@gmail.com>",
  "main": "./dist/wave-ui.umd.js",
  "unpkg": "dist/wave-ui.umd.js",
  "jsdelivr": "dist/wave-ui.umd.js",
  "module": "./dist/wave-ui.es.js",
  "exports": {
    "./dist/wave-ui.css": "./dist/wave-ui.css",
    "./dist/wave-ui.cjs.js": "./dist/wave-ui.cjs.js",
    ".": {
      "import": "./dist/wave-ui.es.js",
      "require": "./dist/wave-ui.umd.js"
    }
  },
  "files": [
    "dist",
    "src/wave-ui"
  ],
  "keywords": [
    "wave ui",
    "ui framework",
    "ui library",
    "vue ui",
    "vue3",
    "vue framework",
    "ui"
  ],
  "license": "MIT",
  "repository": "https://github.com/antoniandre/wave-ui",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base /wave-ui/",
    "build-bundle": "BUNDLE=true vite build && mv ./dist/style.css ./dist/wave-ui.css",
    "serve": "vite preview --root docs --base /wave-ui/",
    "lint": "vite lint"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.vue"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@mdi/font": "^5.9.55",
    "@vitejs/plugin-vue": "^1.9.3",
    "autoprefixer": "^10.3.7",
    "axios": "^0.21.4",
    "core-js": "^3.18.2",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.19.1",
    "font-awesome": "^4.7.0",
    "ghspa": "^1.0.0",
    "gsap": "^3.8.0",
    "ionicons": "^4.6.3",
    "material-design-icons": "^3.0.1",
    "rollup-plugin-delete": "^2.0.0",
    "sass": "^1.42.1",
    "simple-syntax-highlighter": "^1.4.10",
    "splitpanes": "^2.3.8",
    "vite": "^2.6.7",
    "vite-plugin-pug": "^0.3.0",
    "vite-plugin-vue2": "^1.8.2",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.14",
    "vueperslides": "^2.15.2",
    "vuex": "^3.6.2"
  }
}
